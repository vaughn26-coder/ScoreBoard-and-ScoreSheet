<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="900.0" spacing="10" style="-fx-padding: 20;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.scoreboardfinal.HistoryController">

   <ListView fx:id="historyListView" prefHeight="350" prefWidth="380" style="-fx-background-color: transparent; -fx-border-color: red; -fx-border-radius: 10; -fx-border-width: 5;" />
   <HBox alignment="CENTER" prefHeight="82.0" prefWidth="860.0">
      <children>
         <Button fx:id="backButton" mnemonicParsing="false" onAction="#handleBack" prefHeight="30.0" prefWidth="160.0" style="-fx-background-color: transparent; -fx-border-color: red; -fx-border-radius: 10;" text="Back">
            <font>
               <Font name="Agency FB Bold" size="15.0" />
            </font>
            <HBox.margin>
               <Insets right="10.0" />
            </HBox.margin></Button>
         <Button fx:id="removeButton" mnemonicParsing="false" onAction="#handleRemove" prefHeight="30.0" prefWidth="160.0" style="-fx-background-color: transparennt; -fx-border-color: red; -fx-border-radius: 10;" text="Remove">
            <font>
               <Font name="Agency FB Bold" size="15.0" />
            </font></Button>
      </children>
   </HBox>
</VBox>
